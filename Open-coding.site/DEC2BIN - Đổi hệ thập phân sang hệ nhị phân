#include <bits/stdc++.h>
#define ll long long
using namespace std;

int t;
ll n;
stack<int>st;
int main() {
ios_base::sync_with_stdio(false); cin.tie(0); cout.tie(0);
  cin >> t;
  while(t--){
    cin >> n;
    while(n!=0) st.push(n%2), n/=2;
    while(!st.empty()) {cout<<st.top(); st.pop();}
    cout<<'\n';
  }

return 0;
}
